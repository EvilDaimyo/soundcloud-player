{
	"manifest_version": 2,
	"name": "SoundCloud Player",
	"short_name": "SC Player",
	"description": "for soundcloud heavy users",
	"version": "1.0.0",
	"homepage_url": "https://github.com/S4WA/soundcloud-player",
	"permissions": [
		"storage"
	],
	"content_scripts": [ {
		"all_frames": true,
		"js": [
			"contents.js"
		],
		"matches": [ "*://soundcloud.com/*" ],
		"run_at": "document_end"
	} ],
	"browser_action": {
		"default_popup": "popup/popup.html",
		"default_icon": "icon.png"
	},
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	}
}